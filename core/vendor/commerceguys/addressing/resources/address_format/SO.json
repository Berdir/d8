{
    "locale": "so",
    "format": "%recipient\n%organization\n%addressLine1\n%addressLine2\n%locality, %administrativeArea %postalCode",
    "required_fields": [
        "addressLine1",
        "locality",
        "administrativeArea"
    ],
    "uppercase_fields": [
        "addressLine1",
        "addressLine2",
        "locality",
        "administrativeArea"
    ],
    "administrative_area_type": "province",
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{5}"
}